@page
@model RegistrationformAssignment.Pages.FormApplicationsModel
@{
}

<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">Voornaam</th>
            <th scope="col">Achternaam</th>
            <th scope="col">Email</th>
            <th scope="col">Telefoonnummer</th>
            <th scope="col">Woonplaats</th>
            <th scope="col">Vakgebied</th>
            <th scope="col">Opleiding</th>
            <th scope="col">Werkervaring</th>
            <th scope="col">Gewenste functie</th>
            <th scope="col">Aanleiding</th>
            <th scope="col">Locatie</th>
            <th scope="col">Opmerkingen</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var forms in Model.Registrationforms)
        {
        <tr>
            <td>@forms.FirstName</td>
            <td>@forms.LastName</td>
            <td>@forms.Email</td>
            <td>@forms.PhoneNumber</td>
            <td>@forms.Residence</td>
            <td>@forms.FieldOfExpertise</td>
            <td>@forms.Education</td>
            <td>@forms.WorkExperience</td>
            <td>@forms.DesiredFunction</td>
            <td>@forms.JobApplicationReason</td>
            <td>@forms.DesiredLocation</td>
            <td>@forms.FormComments</td>
        </tr>
        }
    </tbody>
</table>

